class Solution {
    public int[] runningSum(int[] nums) {
        int z=0;
        int ans[]=new int[nums.length];
        int a[]=new int[nums.length];
        for(int i=0;i<nums.length;i++)
        {
          
              
              z+=nums[i];
              ans[i]=z;     
        }
        return ans;

            
    }
}
