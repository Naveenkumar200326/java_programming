class Solution {
    public int[] twoSum(int[] nums, int target) {
        int x=nums.length;
        int res[]=new int[2];
        for(int i=0;i<x;i++)
        {
            for(int j=1;j<x;j++)
            {
                if(i!=j)
                {
            int a=nums[i]+nums[j];
            if(a==target)
            {
                res[0]=i;
                res[1]=j;
            }
                }
            }
        }
        return res;
    }
}
