public class Main
{
    public static void main(String[] args) {
        int nums1[]={1,2,3,4,5};
        int nums2[]={10,20,30,40,50};

        int res[]=new int[(nums2.length*2)];
        for(int i=0;i<nums1.length;i++)
        {
            res[i]=nums1[i];
            res[(nums1.length)+i]=nums2[i];
        }
        for(int i=0;i<res.length;i++)
        {
            System.out.print(res[i]+" ");
        }


    }
}
