class Solution {
    public void moveZeroes(int[] nums) {
        
        int y=0;
       
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]!=0)
            {
                nums[y++]=nums[i];
            }
        }
        while(y<nums.length)
        {
            nums[y++]=0;
        }
    }
}
