class Solution {
    public int numIdenticalPairs(int[] nums) {
        int x=nums.length;
        int y=0;
        for(int i=0;i<x;i++)
        {
            for(int j=0;j<x;j++)
            {
                if (i<j && nums[i]==nums[j])
                {
                    y=y+1;
                }
            }

        }
        return y;
    }
}
