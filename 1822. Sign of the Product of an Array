class Solution {
    public int arraySign(int[] nums) {
        int x=nums.length;
        double m=1;
        int res=0;
        for(int i=0;i<x;i++)
        {
            m=m*nums[i];
        }
        if(m>0)
        {
            res=1;
        }
        else if(m<0)
        {
            res=-1;
        }
        else
        {
            res=0;
        }
        return res;
    }
}
